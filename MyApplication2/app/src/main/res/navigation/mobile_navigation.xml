<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.myapplication.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_orderPageOneFragment"
            app:destination="@id/nav_orderPageOneFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_account"
        android:name="com.example.myapplication.ui.account.AccountFragment"
        android:label="@string/account"
        tools:layout="@layout/fragment_account" >
        <action
            android:id="@+id/action_nav_account_to_orderHistoryFragment"
            app:destination="@id/nav_orderHistoryFragment" />
        <argument
            android:name="email" />
        <argument
            android:name="password" />
        <argument
            android:name="name" />
        <argument
            android:name="phone" />
        <argument
            android:name="address" />
        <argument
            android:name="cardNum" />
        <argument
            android:name="cardExp" />
        <argument
            android:name="cardCVV" />
        <action
            android:id="@+id/action_nav_account_to_nav_login"
            app:destination="@id/nav_login" />
        <argument android:name="cardName" />
    </fragment>

    <fragment
        android:id="@+id/nav_menu"
        android:name="com.example.myapplication.ui.menu.MenuFragment"
        android:label="@string/menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_nav_menu_to_beansFragment"
            app:destination="@id/nav_beansFragment" />
        <action
            android:id="@+id/action_nav_menu_to_CYOFragment"
            app:destination="@id/nav_CYOFragment" />
        <action
            android:id="@+id/action_nav_menu_to_preMadeFragment"
            app:destination="@id/nav_preMadeFragment" />
        <action
            android:id="@+id/action_nav_menu_to_nav_orderPageOneFragment"
            app:destination="@id/nav_orderPageOneFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_beansFragment"
        android:name="com.example.myapplication.ui.menu.BeansFragment"
        android:label="Beans"
        tools:layout="@layout/fragment_beans" >
        <action
            android:id="@+id/action_beansFragment_to_nav_menu"
            app:destination="@id/nav_menu" />
        <action
            android:id="@+id/action_beansFragment_to_nav_orderPageOneFragment"
            app:destination="@id/nav_orderPageOneFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_CYOFragment"
        android:name="com.example.myapplication.ui.menu.CYOFragment"
        android:label="Create Your Own"
        tools:layout="@layout/fragment_c_y_o" >
        <action
            android:id="@+id/action_CYOFragment_to_nav_menu"
            app:destination="@id/nav_menu" />
        <action
            android:id="@+id/action_CYOFragment_to_nav_orderPageOneFragment"
            app:destination="@id/nav_orderPageOneFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_preMadeFragment"
        android:name="com.example.myapplication.ui.menu.PreMadeFragment"
        android:label="Pre-Made"
        tools:layout="@layout/fragment_pre_made" >
        <action
            android:id="@+id/action_preMadeFragment_to_nav_menu"
            app:destination="@id/nav_menu" />
        <action
            android:id="@+id/action_preMadeFragment_to_nav_orderPageOneFragment"
            app:destination="@id/nav_orderPageOneFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_contact_us"
        android:name="com.example.myapplication.ui.contact.ContactUsFragment"
        android:label="Contact Us"
        tools:layout="@layout/fragment_contact_us" >
        <action
            android:id="@+id/action_nav_contact_us_to_nav_orderPageOneFragment"
            app:destination="@id/nav_orderPageOneFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_orderPageOneFragment"
        android:name="com.example.myapplication.ui.order.OrderPageOneFragment"
        android:label="Order"
        tools:layout="@layout/fragment_order_page_one" >
        <action
            android:id="@+id/action_orderPageOneFragment_to_orderPageTwoFragment"
            app:destination="@id/nav_orderPageTwoFragment" />
        <argument android:name="userPhone" />
    </fragment>
    <fragment
        android:id="@+id/nav_orderPageTwoFragment"
        android:name="com.example.myapplication.ui.order.OrderPageTwoFragment"
        android:label="Order"
        tools:layout="@layout/fragment_order_page_two" >
        <action
            android:id="@+id/action_orderPageTwoFragment_to_orderPageOneFragment"
            app:destination="@id/nav_orderPageOneFragment" />
        <argument android:name="orderDate" />
        <argument android:name="size" />
        <argument android:name="coffee" />
        <argument android:name="cream" />
        <argument android:name="flavor" />
        <action
            android:id="@+id/action_nav_orderPageTwoFragment_to_saveOrderLoginFragment"
            app:destination="@id/saveOrderLoginFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_orderHistoryFragment"
        android:name="com.example.myapplication.ui.account.OrderHistoryFragment"
        android:label="Order History"
        tools:layout="@layout/fragment_order_history" >
        <action
            android:id="@+id/action_orderHistoryFragment_to_nav_account"
            app:destination="@id/nav_account" />
        <action
            android:id="@+id/action_orderHistoryFragment_to_nav_orderPageOneFragment"
            app:destination="@id/nav_orderPageOneFragment" />
        <argument android:name="userPhone" />
    </fragment>
    <activity
        android:id="@+id/mainActivity2"
        android:name="com.example.myapplication.MainActivity"
        android:label="app_bar_main"
        tools:layout="@layout/app_bar_main" />
    <fragment
        android:id="@+id/nav_login"
        android:name="com.example.myapplication.ui.home.LoginFragment"
        android:label="Account"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_nav_login_to_nav_account2"
            app:destination="@id/nav_account" />
    </fragment>
    <fragment
        android:id="@+id/saveOrderLoginFragment"
        android:name="com.example.myapplication.ui.order.SaveOrderLoginFragment"
        android:label="Save Order"
        tools:layout="@layout/fragment_save_order_login">
        <action
            android:id="@+id/action_saveOrderLoginFragment_to_nav_home"
            app:destination="@id/nav_home" />
        <argument android:name="date" />
        <argument android:name="size" />
        <argument android:name="coffee" />
        <argument android:name="cream" />
        <argument android:name="flavor" />
    </fragment>
</navigation>